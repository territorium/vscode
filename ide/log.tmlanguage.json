{
    "scopeName": "source.output",
    "name": "SYSLOG",
    "patterns": [
        {
            "name": "syslog.log",
            "match": "(\\[.+\\]\\:\\s*)?[^|]+\\|\\s*(FATAL|ERROR|WARN|INFO|CONFIG|DEBUG|TRACE).*",
            "captures": {
                "1": {
                    "name": "syslog-proc"
                },
                "2": {
                    "name": "syslog-level",
                    "patterns": [
                        {
                            "match": "FATAL",
                            "name": "syslog-level.FATAL"
                        },
                        {
                            "match": "ERROR",
                            "name": "syslog-level.ERROR"
                        },
                        {
                            "match": "WARN",
                            "name": "syslog-level.WARN"
                        },
                        {
                            "match": "INFO",
                            "name": "syslog-level.INFO"
                        },
                        {
                            "match": "CONFIG",
                            "name": "syslog-level.CONFIG"
                        },
                        {
                            "match": "DEBUG",
                            "name": "syslog-level.DEBUG"
                        },
                        {
                            "match": "TRACE",
                            "name": "syslog-level.TRACE"
                        }
                    ]
                }
            }
        }
    ]
}